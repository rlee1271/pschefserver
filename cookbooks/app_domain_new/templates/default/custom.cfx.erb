; a connection to the remote search domain:
;	* Port used by this application server to communicate to the remote search 	
;	  server
;	* The ip address of the remote search server as well as the port it uses to
;	  communicate with this application server and
;	* Finally, the search server's DomainID. It can be found in the
;	  [Domain Settings] section of the search domain's configuration file
;	  (pssrchsrv.cfg).
; For instance,
;    Application Server Port=7777
;    Remote Search Server Credentials=SRCHSERV|srchsrv_computer:7778
; indicates that a Search Domain with Domain ID 'SRCHSERV' is running on 'srchsrv_computer', listening on port 7778
;; ld="The port on which the Application Server Domains Gateway will listen for communications from the remote Search domain", rl="Y", dt="n", tip="This value must match the Search Server Port in the Search domain configuration", rq="N"
Application Server Port=7777
;; ld="The Domain ID and machine:port combination representing the remote Search domain.  The domain id must precede the machine:port combination and be separated by a vertical tab | character.  The machine name should be a fully qualified domain name, e.g. myserver.oracle.com", rl="Y", tip="Enter as [Domain ID]+|+[machine:port]. Port refers to the Search Server Port. If multiple search servers, entries should be comma separated,e.g. TESTSERV|mymachine:8888,PTDOMN|mymachine2:9999", rq="N"
Remote Search Server Credentials=

[Search Indexes]
;=========================================================================
; Text-Search index settings
;=========================================================================
;; ssp="", dt="g", rq="N", ld="This is an explicit search index path for this index name"
; Search indexes can be given alternate locations if there is an entry here.
; Entries look like: IndexName=fs location (ie EMPLOYEE=c:\temp)

[PSRENSRV]
;=========================================================================
; Event notification server settings
;=========================================================================
; REN Server log trace level.  Use Error, Warning, Notice, or Debug.
;; sd="Log Level", ld="REN Server log trace level", dt="d", rq="N"
log_severity_level=Warning

; REN Server buffer size for HTTP output.  Must not exceed 65536.
;; sd="IO Buffer Size", ld="REN Server HTTP buffer size output", dt="n", rq="N"
io_buffer_size=8192

; REN Servers place these values in the database when creating
; out-of-the-box initialization parameters.
; If commented out, the values "7180" and "peoplesoft.com" are used.
;; sd="HTTP Port", ld="Remote Event Notification Server HTTP Port", rq="N", dt="n"
default_http_port=7180
;; sd="HTTPS Port", ld="Remote Event Notification Server HTTPS Port", rq="N", dt="n"
default_https_port=7143
;; sd="Authentication Token", ld="Remote Event Notification Server Authentication Token", rq="N", tip="This value should match the authentication token provided during PIA installation"
default_auth_token=peoplesoft.com


[PSPPMSRV]
;=========================================================================
; Settings for PSPPMSRV
;=========================================================================

;-------------------------------------------------------------------------
; UBBGEN settings
; Since Performance Collators do not advertise services Max and Min
; Instances must always be equal
;; sd="Minimum Instances", ld = "Minimum number of process instances started upon domain boot", rl = "Y", dt = n, rq="N"
Min Instances=1
;; sd="Maximum Instances", ld = "Maximum number of process instances that will be spawned at peak time", rl = "Y", tip = "Set this value equal to Min Instances to disable spawning", dt = n, rq="N"
Max Instances=1
;If PPM registration fails while monitoring Application Server is configured to have multiple IPs, IP address needs to be set explicitly
;; sd="Monitoring Server's IPV4 address", rq="N", ld="When Monitoring server is configured to have multiple IPs, this value might be required"
listening_mach_IP=
;

[LDAP]
;=========================================================================
; General settings for LDAP
;=========================================================================
; Should the referrals be followed in the Directory server?
; Y - follow referrals.
; N - Do not follow referrals.
;; sd="Follow Referrals", ld="Determines whether the referrals have to be followed in the Directory Server. By default, the referrals will be followed", dt="c", dy="Y", rq="N"
Follow Referrals=Y
;-------------------------------------------------------------------------
; LDAP Connection TimeOut in seconds
; Range(0-120). 0 - Default TCP Timeout.
;; sd="LDAP Connection TimeOut", ld="Time Limit in seconds for the PT LDAP Client to make a connection with the Directory server. Default is 30 seconds", dt="n", dy="Y", rq="N"
Connection TimeOut=30
;-------------------------------------------------------------------------
; Enable/Disable Connection Pooling
; Y - Enable Connection Pooling.
; N - Disable Connection Pooling.
;; sd="Enable Connection Pooling", ld="Determines whether the connections from the PT LDAP client have to be pooled in the Directory Server. This is disabled by default", dt="c", dy="Y", rq="N"
Connection Pooling=N
;-------------------------------------------------------------------------
;LDAP Connection Pooling TimeOut in seconds. Not Applicable when the connection pooling is disabled.
; Range(0-999999). 0 - No Time Limit.
;; sd="Connection Pooling TimeOut", ld="LDAP Connection Pooling TimeOut in seconds. Not Applicable when the connection pooling is disabled. Default is 300 seconds", dt="n", dy="Y", rq="N"
Connection Pooling TimeOut=300
;-------------------------------------------------------------------------
; Should the LDAP pick up any changes in the ssl certificates?
; Y - Changes will be in effect without Appserver reboot.
; N - Appserver reboot needed to pick up the changes.
;; sd="Dynamic Update of Certificates", ld="Determines whether the application server domain has to be bounced to pick up any changes in the certificates.", dt="c", dy="Y", rq="N"
Dynamic Update of Certificates=N
;-------------------------------------------------------------------------
; LDAP Logging detail level
;
; Level     Type of information
; -----     -------------------
;   0       - Suppress Logging
;   1       - Errors and Exceptions (default)
;   2       - Transaction Status
;   3       - Detailed Tracing
; Dynamic change allowed for LogFence
;; sd="LDAP Log Fence", ld="Determines the level of logging for the LDAP module", dt="d", dy="Y", rq="N"
;; option(va =    0, sd="Suppress Logging")
;; option(va =    1, sd="Errors and Exceptions (default)")
;; option(va =    2, sd="Transaction Status")
;; option(va =    3, sd="Detailed Tracing")
LDAP LogFence=1

 
