[Trace]
;=========================================================================
; Server Trace settings
;=========================================================================

;-------------------------------------------------------------------------
; SQL Tracing Bitfield
;
; Bit       Type of tracing
; ---       ---------------
; 1         - SQL statements
; 2         - SQL statement variables
; 4         - SQL connect, disconnect, commit and rollback
; 8         - Row Fetch (indicates that it occurred, not data)
; 16        - All other API calls except ssb
; 32        - Set Select Buffers (identifies the attributes of columns
;             to be selected).
; 64        - Database API specific calls
; 128       - COBOL statement timings
; 256       - Sybase Bind information
; 512       - Sybase Fetch information
; 1024      - SQL Informational Trace
; 4096      - Manager information
; 8192      - Mapcore information
; Dynamic change allowed for TraceSql and TraceSqlMask
;; sd="SQL Tracing Bitfield", dt="c", dr="m", dy="Y", tab("TraceSql")
;; option(va =    1, sd="SQL statements")
;; option(va =    2, sd="SQL statement variables")
;; option(va =    4, sd="SQL connect, disconnect, commit and rollback")
;; option(va =    8, sd="Row Fetch")
;; option(va =   16, sd="All other API calls except ssb")
;; option(va =   32, sd="Set Select Buffers")
;; option(va =   64, sd="Database API specific calls")
;; option(va =  128, sd="COBOL statement timings")
;; option(va =  256, sd="Sybase Bind information")
;; option(va =  512, sd="Sybase Fetch information")
;; option(va = 1024, sd="SQL Informational Trace")
;; option(va = 4096, sd="Manager information")
;; option(va = 8192, sd="Mapcore information")
TraceSql=0
;; sd="SQL Tracing Mask", dt = n, dy = Y, ld="Logging level ceiling for SQL tracing for individual clients"
TraceSqlMask=12319

