[PSTOOLS]
;=========================================================================
; General settings for PSTOOLS
;=========================================================================
; This will suppress SQL error dialogs. This option helps prevent
; SQL injection attacks by only writing SQL errors to the log files
; (and not displaying them to a user.)
Suppress SQL Error=1

;Uncomment this to specify an alternate directory to search for Interface Drivers.
;Business Interlink Driver Directory=

;Set the value to 1 to Enable PPM Agents,0 to disable PPM Agents
EnablePPM Agent=0

;To add directories or jar files to CLASSPATH for the jvm loaded via JNI, uncomment Add to CLASSPATH=
;and set it equal to the list of elements you would like to use.
;The classpath elements need to be seperated using the path seperator specific to the platform.
;For Example: Add to CLASSPATH=/usr/class:/home/user/class
;Add to CLASSPATH=

;To pass java options to the jvm loaded via JNI, uncomment JavaVM Options=
;and set it equal to the list of parameters you would like to use.
;The options need to be a blank space separated list of valid java options.
;Consult your JRE vendor for valid java options.
;For example: JavaVM Options=-Xmx256m -Xms128m
JavaVM Options=-Dxdo.ConfigFile=%PS_HOME%/appserv/xdo.cfg

;If the application server is behind a proxy server,
;set "Proxy Host=" to the domain name of the proxy server (proxy.peoplesoft.com)
;set "Proxy Port=" to the port number of the proxy server (80)
Proxy Host=

Proxy Port=

;set "Non Proxy Hosts=" to a list of hosts that should be connected to directly
;and not through proxy server. The value can be a list of hosts, each
;seperated by a |, and in addition a wildcard character (*) can be used for
;matching. For example: Non Proxy Hosts=*.foo.com|localhost. The maximum
;length of this string cannot exceed 1024 characters.
Non Proxy Hosts=

;To set the DISPLAY environment variable on headless UNIX boxes running applications
;with chart graphics. Xvfb must also be started and configured to use this value.
;For example, Display=localhost:10.0
;Display=

;-------------------------------------------------------------------------
; IMPORTANT NOTE: THIS SETTING IS APPLICABLE FOR ALL PLATFORMS!
; The "Character Set" setting specifies the character set and encoding
; used for naming files.  If this setting is commented out, defaults
; will be used as follows:
;   Windows: The system active code page (CP_ACP)
;   UNIX:    latin1
;   z/OS:    ccsid1047
; To see the available character encodings, run 'psuniconv -usage'
;Character Set=latin1

;-------------------------------------------------------------------------
; WINDOWS PLATFORM ONLY:
; Suppress App Error Box: After an application error occurs, suppress the
; console's Application Error dialog box.
Suppress App Error Box=Y

;-------------------------------------------------------------------------
; DbFlags Bitfield
;
; Bit       Flag
; ---       ----
; 1         - Ignore metaSQL to update database statistics(shared with COBOL)
; 2         - Ignore Truncate command for DB2Unix, use Delete instead
; 4         - Disable Second DB Connection
; 8         - Disable Persistent Secondary DB Connection
; 16        - not yet allocated
; 32        - not yet allocated
; 64        - not yet allocated
; 128       - not yet allocated
; 256       - not yet allocated
DbFlags=8


[Integration Broker]
;=========================================================================
; General settings for the Integration Broker
;=========================================================================
; Minimum size of message data for synchronous handler to enable compression.
Min Message Size For Compression=100000

; Thread Pool Size used by SyncRequest PeopleCode event.
; The Minimum value is 1 and maximum allowable is 20.
Thread Pool Size=5

[Search]
;=========================================================================
; Text-Search settings
;=========================================================================
; If no option is provided, the application server will automatically
; select the preferred mechanism for the deployment platform.
; 1 - Search module bound to an application server
; 2 - Search module in same machine as the application server
; 3 - Search module bound to remote search domain
Deployment Type=

; These settings are used to delegate text searches to a search server in a remote
; search domain. You must specify the following information in order to establish
; a connection to the remote search domain:
;	* Port used by this application server to communicate to the remote search 	
;	  server
;	* The ip address of the remote search server as well as the port it uses to
;	  communicate with this application server and
;	* Finally, the search server's DomainID. It can be found in the
;	  [Domain Settings] section of the search domain's configuration file
;	  (pssrchsrv.cfg).
; For instance,
;    Application Server Port=7777
;    Remote Search Server Credentials=SRCHSERV|srchsrv_computer:7778
; indicates that a Search Domain with Domain ID 'SRCHSERV' is running on 'srchsrv_computer', listening on port 7778
Application Server Port=7777
Remote Search Server Credentials=

[Search Indexes]
;=========================================================================
; Text-Search index settings
;=========================================================================
; Search indexes can be given alternate locations if there is an entry here.
; Entries look like: IndexName=fs location (ie EMPLOYEE=c:\temp)

[PSRENSRV]
;=========================================================================
; Event notification server settings
;=========================================================================
; REN Server log trace level.  Use Error, Warning, Notice, or Debug.
log_severity_level=Warning

; REN Server buffer size for HTTP output.  Must not exceed 65536.
io_buffer_size=8192

; REN Servers place these values in the database when creating
; out-of-the-box initialization parameters.
; If commented out, the values "7180" and "peoplesoft.com" are used.
default_http_port=7180
default_https_port=7143
default_auth_token=peoplesoft.com


[PSPPMSRV]
;=========================================================================
; Settings for PSPPMSRV
;=========================================================================

;-------------------------------------------------------------------------
; UBBGEN settings
; Since Performance Collators do not advertise services Max and Min
; Instances must always be equal
Min Instances=1
Max Instances=1
