;=========================================================================
; Trace settings
;=========================================================================
; SQL Tracing Bitfield
;
; Bit       Type of tracing
; ---       ---------------
; 1         - SQL statements
; 2         - SQL statement variables
; 4         - SQL connect, disconnect, commit and rollback
; 8         - Row Fetch (indicates that it occurred, not data)
; 16        - All other API calls except ssb
; 32        - Set Select Buffers (identifies the attributes of columns
;             to be selected).
; 64        - Database API specific calls
; 128       - COBOL statement timings
; 256       - Sybase Bind information
; 512       - Sybase Fetch information
; 1024      - SQL Informational Trace
; Dynamic change allowed for TraceSql and TraceSqlMask
;; sd="SQL Tracing Bitfield", dt="c", dr="m", dy="Y", tab("TraceSQL")
;; option(va =    1, sd="SQL statements")
;; option(va =    2, sd="SQL statement variables")
;; option(va =    4, sd="SQL connect, disconnect, commit and rollback")
;; option(va =    8, sd="Row Fetch")
;; option(va =   16, sd="All other API calls except ssb")
;; option(va =   32, sd="Set Select Buffers")
;; option(va =   64, sd="Database API specific calls")
;; option(va =  128, sd="COBOL statement timings")
;; option(va =  256, sd="Sybase Bind information")
;; option(va =  512, sd="Sybase Fetch information")
;; option(va = 1024, sd="SQL Informational Trace")
TraceSQL=0

;-------------------------------------------------------------------------
; PeopleCode Tracing Bitfield
;
; Bit       Type of tracing
; ---       ---------------
; 1         - Trace Evaluator instructions  (not recommended)
; 2         - List Evaluator program        (not recommended)
; 4         - Show assignments to variables
; 8         - Show fetched values
; 16        - Show stack
; 64        - Trace start of programs
; 128       - Trace external function calls
; 256       - Trace internal function calls
; 512       - Show parameter values
; 1024      - Show function return value
; 2048      - Trace each statement in program  (recommended)
; Dynamic change allowed for TracePC 
;; sd="PeopleCode Tracing Bitfield", dt="c", dr="m", tip="Levels 1 and 2 and 2048 not recommended", dy="Y", tab("TracePC")
;; option(va =    1, sd="Trace Evaluator instructions")
;; option(va =    2, sd="List Evaluator program")
;; option(va =    4, sd="Show assignments to variables")
;; option(va =    8, sd="Show fetched values")
;; option(va =   16, sd="Show stack")
;; option(va =   64, sd="Trace start of programs")
;; option(va =  128, sd="Trace external function calls")
;; option(va =  256, sd="Trace internal function calls")
;; option(va =  512, sd="Show parameter values")
;; option(va = 1024, sd="Show function return value")
;; option(va = 2048, sd="Trace each statement in program (recommended)")
TracePC=0

;-------------------------------------------------------------------------
; AE Tracing Bitfield
;
; Bit       Type of tracing
; ---       ---------------
; 1         - Trace STEP execution sequence to AET file
; 2         - Trace Application SQL statements to AET file
; 4         - Trace Dedicated Temp Table Allocation to AET file
; 8         - not yet allocated
; 16        - not yet allocated
; 32        - not yet allocated
; 64        - not yet allocated
; 128       - Timings Report to AET file
; 256       - Method/BuiltIn detail instead of summary in AET Timings Report
; 512       - not yet allocated
; 1024      - Timings Report to tables
; 2048      - DB optimizer trace to file
; 4096      - DB optimizer trace to tables
;; sd="AE Tracing Bitfield", ld="Application Engine Tracing Bitfield", dt="c", dr="m", dy="Y", tab("TraceAE")
;; option(va =     1, sd="Trace STEP execution sequence to AET file")
;; option(va =     2, sd="Trace Application SQL statements to AET file")
;; option(va =     4, sd="Trace Dedicated Temp Table Allocation to AET file")
;; option(va =     8, sd="not yet allocated")
;; option(va =    16, sd="not yet allocated")
;; option(va =    32, sd="not yet allocated")
;; option(va =    64, sd="not yet allocated")
;; option(va =   128, sd="Timings Report to AET file")
;; option(va =   256, sd="Method/BuiltIn detail instead of summary in AET Timings Report")
;; option(va =   512, sd="not yet allocated")
;; option(va =  1024, sd="Timings Report to tables")
;; option(va =  2048, sd="DB optimizer trace to file")
;; option(va =  4096, sd="DB optimizer trace to tables")
TraceAE=0

;-------------------------------------------------------------------------
