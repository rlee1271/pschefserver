[Cache Settings]
;=========================================================================
; Settings for Batch Tools that use Cache, eg Application Engine
;=========================================================================
;-------------------------------------------------------------------------
; Parent of cache directory. Do not share with Application Servers.
CacheBaseDir=%PS_SERVDIR%
;-----------------------------------------------------------------------
; MaxCacheMemory - controls cache memory pruning
;  0        cache memory pruning disabled
;  >0       max size of memory cache in MBytes
MaxCacheMemory=0
;-----------------------------------------------------------------------
; EnableDBCache - When set to Y, Cache is accessed from DB, rather than File-System.
; When this setting is enabled, the settings EnableServerCaching, ServerCacheMode and CacheBaseDir are ignored.
; Y  DB-Cache is enabled
; N  DB-Cache is disabled
;EnableDBCache=

;-----------------------------------------------------------------------
; Cache Repair Settings
;-----------------------------------------------------------------------
; EnableCacheRepair - When set to Y, cache repair activities will execute.
;                     When this setting is disabled, the settings AdjustMaxCacheMem, 
;                     ExcessivePruningThreshold, CacheMissThreshold, and 
;                     MaxRepairCollection are ignored.
; Y  cache repair is enabled - default
; N  cache repair is disabled
; Dynamic change allowed for EnableCacheRepair
EnableCacheRepair=Y
;-----------------------------------------------------------------------
; AdjustMaxCacheMem - Controls how much to adjust the internal setting for MaxCacheMem 
;                     if excessive pruning is detected. This is unused if MaxCacheMem=0.
;  0        disables adjusting internal setting
;  >0       #MBytes by which to increase the internal setting
; Dynamic change allowed for AdjustMaxCacheMem
AdjustMaxCacheMem=5
;-----------------------------------------------------------------------
; ExcessivePruningThreshold - Threshold for ratio of pruned objects to objects fetched  
;                             in a service request. Once the threshold is reached the 
;                             internal setting for MaxCacheMem is incremented by the 
;                             amount in AdjustMaxCacheMem. This is unused if MaxCacheMem=0.
;                             Expressed as a percentage.
; Dynamic change allowed for ExcessivePruningThreshold
ExcessivePruningThreshold=80
;-----------------------------------------------------------------------
; CacheMissThreshold - Threshold for the number of times an object is found to be invalid
;                      in cache before deleting the object from cache, forcing a refresh.
; Dynamic change allowed for CacheMissThreshold
CacheMissThreshold=5
;-----------------------------------------------------------------------
; MaxRepairCollection - Maximum size of the memory data collection for cache repair
; Dynamic change allowed for MaxRepairCollection
MaxRepairCollection=5

